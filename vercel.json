{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/auth-callback",
            "destination": "/dist/public/auth-callback.html"
        },
        {
            "source": "/(.*)",
            "destination": "/dist/public/index.html"
        }
    ]
}